{
  "name": "quick-credit",
  "version": "1.0.0",
  "description": "An online lending platform that provides short term soft loans to individuals",
  "main": "app.js",
  "scripts": {
    "start-dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "start": "cross-env-shell NODE_ENV=production \"npm run build && node ./build/app.js\"",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha ./src/test/*.js --require @babel/register --timeout 5000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build-src": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-src",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elikeyz/quick-credit.git"
  },
  "keywords": [
    "Credit",
    "Loan",
    "Lending"
  ],
  "author": "Elijah Enuem-Udogu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elikeyz/quick-credit/issues"
  },
  "homepage": "https://github.com/elikeyz/quick-credit#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.0.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "express": "^4.16.4"
  }
}
